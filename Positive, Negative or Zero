import java.util.Scanner;

public class Posnegzero {
	public static void main(String[] args) {
		Scanner kbd=new Scanner(System.in);

				int n=kbd.nextInt();
				if(n>0) {
					System.out.println("POSTIVE");
				}

				else if(n<0){
					System.out.println("NEGATIVE");
				}

				else {
					System.out.println("ZERO");
				}
			}
		}
  OUTPUT:
  9
  POSTIVE
  -5
  NEGATIVE
  0
  ZERO
