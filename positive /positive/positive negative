import java.util.Scanner;

public class negativepostive {

	public static void main(String[] args) {

		Scanner kbd=new Scanner(System.in);

		int n=kbd.nextInt();
		if(n>0) {
			System.out.println("POSTIVE");
		}

		else if(n<0){
			System.out.println("NEGATIVE");
		}

		else {
			System.out.println("ZERO");
		}
	}
 OUTPUT:
 8
 postive
 -11
 negative
 0
 zero
